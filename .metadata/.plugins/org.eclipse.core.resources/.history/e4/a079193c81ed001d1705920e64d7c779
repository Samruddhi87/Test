package com.example.pojo;

import java.util.List;

public class Category {

	private String name;
	private Long id;
    private List<Product> products;

	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	
	
}
